<launch>
  <node pkg="nodelet" type="nodelet" name="depth_nodelet_manager" args="manager" output="screen" />
  <node pkg="nodelet" type="nodelet" name="laserscan_nodelet"
        args="load depthimage_to_laserscan/DepthImageToLaserScanNodelet depth_nodelet_manager" output="screen">
    <remap from = "image" to = "/guidance/depth/image_raw" />
    <remap from = "/guidance/depth/camera_info" to = "/guidance/left/camera_info" />
  </node>

  <node pkg="nodelet" type="nodelet" name="pointcloud_nodelet"
        args="load depth_image_proc/point_cloud_xyz depth_nodelet_manager" >
    <remap from = "image_rect" to = "/guidance/depth/image_raw" />
    <remap from = "/guidance/depth/camera_info" to = "/guidance/left/camera_info" />
  </node>

  
</launch>
